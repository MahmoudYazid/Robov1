@model IEnumerable<WebApplication1.Models.ODB>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ChooseOrgan</title>
</head>
<body>
    <p>
        ChooseOrgan
    </p>
    @Html.ActionLink("Qdbs", "Index", "Qdbs")
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OrganName)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.OrganName)
                </td>
                <td>
                    @Html.ActionLink("Select", "Chooseeffector", new { OrganName = item.OrganName, SympId = Request.QueryString["SympId"] }) |


                </td>
            </tr>
        }

    </table>
</body>
</html>
