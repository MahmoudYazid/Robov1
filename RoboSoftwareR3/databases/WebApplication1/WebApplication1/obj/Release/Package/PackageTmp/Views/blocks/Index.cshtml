@model IEnumerable<WebApplication1.Models.block>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "ChooseSymptom")
        @Html.ActionLink("Qdbs", "Index", "Qdbs")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SympId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.effectorName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.place)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.type_effector)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EffectorId)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SympId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.effectorName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.place)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.type_effector)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EffectorId)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.blockId }) |
                @Html.ActionLink("Details", "Details", new { id=item.blockId }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.blockId })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
