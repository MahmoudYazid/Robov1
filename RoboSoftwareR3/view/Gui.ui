<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="ttk.Labelframe" id="labelframe8">
    <property name="height">200</property>
    <property name="text" translatable="yes">Main
</property>
    <property name="width">200</property>
    <layout manager="pack">
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Combobox" id="ChoicesListComponent" named="True">
        <property name="width">100</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="GiveBtn" named="True">
        <property name="state">normal</property>
        <property name="text" translatable="yes">Give</property>
        <property name="width">20</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">200</property>
          <property name="pady">10</property>
          <property name="row">3</property>
          <property name="sticky">sw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label2">
        <property name="text" translatable="yes">Choose drug or disease
</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="ShowMeBtn" named="True">
        <property name="state">normal</property>
        <property name="text" translatable="yes">Show me </property>
        <property name="width">30</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">0</property>
          <property name="pady">10</property>
          <property name="row">3</property>
          <property name="sticky">sw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="ResetBtn" named="True">
        <property name="state">normal</property>
        <property name="text" translatable="yes">reset</property>
        <property name="width">20</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">340</property>
          <property name="pady">10</property>
          <property name="row">3</property>
          <property name="sticky">sw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="ShowScreenComponant" named="True">
        <property name="height">10</property>
        <property name="setgrid">false</property>
        <property name="state">normal</property>
        <property name="tabstyle">tabular</property>
        <property name="width">100</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
